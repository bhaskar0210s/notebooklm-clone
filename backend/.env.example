# Provider Selection
# Set to "true" to use Ollama, "false" or unset to use Vertex AI (default: false)
USE_OLLAMA=false

# Supabase Configuration (Required)
SUPABASE_URL=your-supabase-url-here
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key-here

# Ollama Configuration (Required when USE_OLLAMA=true)
OLLAMA_BASE_URL=http://localhost:11434

# Google Cloud Vertex AI Configuration (Required when USE_OLLAMA=false)
GOOGLE_CLOUD_PROJECT_ID=your-gcp-project-id-here
GOOGLE_CLOUD_LOCATION=us-central1
# Optional: Path to service account JSON file (for local development)
# If not set, will use Application Default Credentials (ADC)
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json

# Server Configuration
PORT=2024

# Optional: LangSmith
LANGSMITH_API_KEY=your-langsmith-api-key-here
